{
  "version": 2,
  "builds": [
    {
      "src": "django02",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "core/wsgi.py"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/"}]
}
